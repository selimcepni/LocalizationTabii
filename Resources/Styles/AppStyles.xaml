        </Style>

        <sf:LabelStyle
                x:Key="lightThemeLabelStyle"
                FontSize="{OnIdiom 18,
                                   Desktop=24}"
                TextColor="{StaticResource DarkOnLightBackground}" />
        <sf:LabelStyle
                x:Key="darkThemeLabelStyle"
                FontSize="{OnIdiom 18,
                                   Desktop=24}"
                TextColor="{StaticResource LightOnDarkBackground}" />

        <Style x:Key="FormLabelTitleStyle" TargetType="Label">
                <Setter Property="FontSize" Value="14" />
                <Setter Property="FontAttributes" Value="Bold" />
                <Setter Property="TextColor" Value="{StaticResource Gray400}" />
                <Setter Property="Margin" Value="0,0,0,4" />
        </Style>
        <Style TargetType="sf:SfTextInputLayout">
                <Setter Property="ContainerType" Value="Outlined" />
                <Setter Property="ContainerBackground" Value="Transparent" />
                <Setter Property="Stroke" Value="{AppThemeBinding Light={StaticResource DarkOnLightBackground}, Dark={StaticResource LightOnDarkBackground}}" />
                <Setter Property="HintLabelStyle" Value="{AppThemeBinding Light={StaticResource lightThemeLabelStyle}, Dark={StaticResource darkThemeLabelStyle}}" />
        </Style>


        <Style x:Key="ShimmerCustomViewStyle" TargetType="BoxView">
                <Setter Property="BackgroundColor" Value="Gray" />
                <Setter Property="HorizontalOptions" Value="FillAndExpand" />
                <Setter Property="VerticalOptions" Value="CenterAndExpand" />
        </Style>

        <Style TargetType="pullToRefresh:SfPullToRefresh">
                <Setter Property="TransitionMode" Value="SlideOnTop" />
                <Setter Property="PullingThreshold" Value="100" />
                <Setter Property="ProgressColor" Value="{StaticResource Primary}" />
        </Style>

        <!-- Standardize Edilmiş Buton Style'ları -->
        
        <!-- SECONDARY BUTTON - Geri Dön, İptal butonları -->
        <Style x:Key="SecondaryButtonStyle" TargetType="buttons:SfButton">
                <Setter Property="FontSize" Value="16" />
                <Setter Property="FontAttributes" Value="Bold" />
                <Setter Property="CornerRadius" Value="18" />
                <Setter Property="WidthRequest" Value="160" />
                <Setter Property="HeightRequest" Value="50" />
                <Setter Property="Margin" Value="6,6" />
                <Setter Property="TextColor" Value="White" />
                <Setter Property="Background">
                        <Setter.Value>
                                <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                                        <GradientStop Color="{AppThemeBinding Light={StaticResource Gray400}, Dark={StaticResource Gray600}}" Offset="0.0" />
                                        <GradientStop Color="{AppThemeBinding Light={StaticResource Gray500}, Dark={StaticResource Gray500}}" Offset="1.0" />
                                </LinearGradientBrush>
                        </Setter.Value>
                </Setter>
                <Setter Property="Shadow">
                        <Setter.Value>
                                <Shadow Brush="{AppThemeBinding Light={StaticResource Gray400}, Dark={StaticResource Gray600}}" Opacity="0.3" Radius="12" Offset="0,6" />
                        </Setter.Value>
                </Setter>
        </Style>

        <!-- PRIMARY BUTTON - Ana aksiyon butonları -->
        <Style x:Key="PrimaryButtonStyle" TargetType="buttons:SfButton">
                <Setter Property="FontSize" Value="16" />
                <Setter Property="FontAttributes" Value="Bold" />
                <Setter Property="CornerRadius" Value="18" />
                <Setter Property="WidthRequest" Value="160" />
                <Setter Property="HeightRequest" Value="50" />
                <Setter Property="Margin" Value="6,6" />
                <Setter Property="TextColor" Value="White" />
                <Setter Property="Background">
                        <Setter.Value>
                                <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                                        <GradientStop Color="{StaticResource LightPrimaryColor}" Offset="0.0" />
                                        <GradientStop Color="{StaticResource LightSecondaryColor}" Offset="1.0" />
                                </LinearGradientBrush>
                        </Setter.Value>
                </Setter>
                <Setter Property="Shadow">
                        <Setter.Value>
                                <Shadow Brush="{StaticResource LightPrimaryColor}" Opacity="0.4" Radius="14" Offset="0,6" />
                        </Setter.Value>
                </Setter>
        </Style>

        <!-- SUCCESS BUTTON - Devam Et, Çeviriye Başla butonları -->
        <Style x:Key="SuccessButtonStyle" TargetType="buttons:SfButton">
                <Setter Property="FontSize" Value="16" />
                <Setter Property="FontAttributes" Value="Bold" />
                <Setter Property="CornerRadius" Value="18" />
                <Setter Property="WidthRequest" Value="160" />
                <Setter Property="HeightRequest" Value="50" />
                <Setter Property="Margin" Value="6,6" />
                <Setter Property="TextColor" Value="White" />
                <Setter Property="Background">
                        <Setter.Value>
                                <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                                        <GradientStop Color="#27AE60" Offset="0.0" />
                                        <GradientStop Color="#2ECC71" Offset="1.0" />
                                </LinearGradientBrush>
                        </Setter.Value>
                </Setter>
                <Setter Property="Shadow">
                        <Setter.Value>
                                <Shadow Brush="#27AE60" Opacity="0.4" Radius="14" Offset="0,6" />
                        </Setter.Value>
                </Setter>
        </Style>

        <!-- ACCENT BUTTON - Prompt Seç gibi özel vurgu butonları -->
        <Style x:Key="AccentButtonStyle" TargetType="buttons:SfButton">
                <Setter Property="FontSize" Value="16" />
                <Setter Property="FontAttributes" Value="Bold" />
                <Setter Property="CornerRadius" Value="18" />
                <Setter Property="WidthRequest" Value="160" />
                <Setter Property="HeightRequest" Value="50" />
                <Setter Property="Margin" Value="6,6" />
                <Setter Property="TextColor" Value="White" />
                <Setter Property="Background">
                        <Setter.Value>
                                <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                                        <GradientStop Color="{StaticResource LightPrimaryColor}" Offset="0.0" />
                                        <GradientStop Color="{StaticResource LightSecondaryColor}" Offset="1.0" />
                                </LinearGradientBrush>
                        </Setter.Value>
                </Setter>
                <Setter Property="Shadow">
                        <Setter.Value>
                                <Shadow Brush="{StaticResource LightPrimaryColor}" Opacity="0.4" Radius="14" Offset="0,6" />
                        </Setter.Value>
                </Setter>
        </Style>

        <!-- TERTIARY BUTTON - Ana Sayfaya Dön gibi üçüncül butonlar -->
        <Style x:Key="TertiaryButtonStyle" TargetType="buttons:SfButton">
                <Setter Property="FontSize" Value="16" />
                <Setter Property="FontAttributes" Value="Bold" />
                <Setter Property="CornerRadius" Value="18" />
                <Setter Property="WidthRequest" Value="160" />
                <Setter Property="HeightRequest" Value="50" />
                <Setter Property="Margin" Value="6,6" />
                <Setter Property="TextColor" Value="{AppThemeBinding Light={StaticResource Gray600}, Dark={StaticResource Gray300}}" />
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="Stroke" Value="{AppThemeBinding Light={StaticResource Gray400}, Dark={StaticResource Gray500}}" />
                <Setter Property="StrokeThickness" Value="2" />
        </Style>

</ResourceDictionary>
